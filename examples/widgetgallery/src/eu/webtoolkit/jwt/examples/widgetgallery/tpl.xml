<?xml version="1.0" encoding="UTF-8" ?>
<messages>
  <message id="tpl:widget-gallery">
    <div class="wg-wrapper">
      <div class="wg-sidebar">
        <div class="wg-logo">
          ${<if:cpp>}
            ${img src="icons/wt.png"}
          ${</if:cpp>}
          ${<if:java>}
            ${img src="icons/jwt.png"}
          ${</if:java>}
          <h1>Widget Gallery</h1>
        </div>
        ${menu}
      </div>
      <div class="wg-open-menu">
        ${open-menu}
      </div>
      <div class="wg-contents">
        ${contents}
      </div>
      ${contents-cover class="wg-contents-cover"}
    </div>
  </message>
</messages>
