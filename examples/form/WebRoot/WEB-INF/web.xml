<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <servlet>
    <servlet-name>Form</servlet-name>
    <display-name>JWt Form example</display-name>
    <description>JWt Form example</description>
    <servlet-class>eu.webtoolkit.jwt.examples.form.FormMain</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Form</servlet-name>
    <url-pattern>/Form/*</url-pattern>
  </servlet-mapping>
</web-app>

