<project>
  <parent>
    <groupId>org.sonatype.oss</groupId>
    <artifactId>oss-parent</artifactId>
    <version>7</version>
  </parent>

  <modelVersion>4.0.0</modelVersion>
  <groupId>eu.webtoolkit</groupId>
  <artifactId>jwt</artifactId>
  <packaging>jar</packaging>
  <name>Java Web Toolkit</name>
  <version>4.12.2</version>
  <description>Java library for developing web applications</description>
  <url>https://webtoolkit.eu/jwt/</url>

  <licenses>
    <license>
      <name>GNU General Public License, version 2</name>
      <url>http://www.gnu.org/licenses/gpl-2.0.html</url>
      <distribution>repo</distribution>
    </license>
  </licenses>

  <scm>
    <url>https://github.com/emweb/jwt.git</url>
    <connection>https://github.com/emweb/jwt.git</connection>
  </scm>

  <properties>dnl
ifelse(CLASSIFIER,`javax',`
    <fileupload.artifactId>commons-fileupload2-javax</fileupload.artifactId>
    <jakarta.mail-api.version>1.6.7</jakarta.mail-api.version>
    <jakarta.websocket-api.version>1.1.2</jakarta.websocket-api.version>
',`
    <fileupload.artifactId>commons-fileupload2-jakarta-servlet6</fileupload.artifactId>
    <jakarta.mail-api.version>2.1.3</jakarta.mail-api.version>
    <jakarta.websocket-api.version>2.2.0</jakarta.websocket-api.version>
')dnl
  </properties>

  <dependencies>
    <dependency>
      <groupId>org.apache.commons</groupId>
      <artifactId>${fileupload.artifactId}</artifactId>
      <version>2.0.0-M2</version>
    </dependency>

    <dependency>
      <groupId>commons-io</groupId>
      <artifactId>commons-io</artifactId>
      <version>2.16.1</version>
    </dependency>

    <dependency>
      <groupId>org.slf4j</groupId>
      <artifactId>slf4j-api</artifactId>
      <version>1.7.25</version>
    </dependency>

    <dependency>
      <groupId>javax.vecmath</groupId>
      <artifactId>vecmath</artifactId>
      <version>1.5.2</version>
    </dependency>

    <dependency>
      <groupId>jakarta.websocket</groupId>
      <artifactId>jakarta.websocket-api</artifactId>
      <version>${jakarta.websocket-api.version}</version>
    </dependency>
ifelse(CLASSIFIER,`javax',,`
    <dependency>
      <groupId>jakarta.websocket</groupId>
      <artifactId>jakarta.websocket-client-api</artifactId>
      <version>${jakarta.websocket-api.version}</version>
    </dependency>
')
    <dependency>
      <groupId>jakarta.mail</groupId>
      <artifactId>jakarta.mail-api</artifactId>
      <version>${jakarta.mail-api.version}</version>
    </dependency>

    <dependency>
      <groupId>org.antlr</groupId>
      <artifactId>antlr4-runtime</artifactId>
      <version>4.7.2</version>
    </dependency>

    <dependency>
      <groupId>com.google.code.gson</groupId>
      <artifactId>gson</artifactId>
      <version>[2.0, 3.0)</version>
    </dependency>
  </dependencies>
</project>
